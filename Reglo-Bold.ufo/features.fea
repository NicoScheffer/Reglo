# Prefix: Languagesystems
# automatic
languagesystem DFLT dflt;
languagesystem latn dflt;

# automatic
@Uppercase = [ A Acircumflex Agrave B C D E Eacute Ecircumflex Edieresis Egrave F G H I Idieresis J K L M N O P Q R S T U V W X Y Z ];

feature cpsp {
# automatic
pos @Uppercase <5 0 10 0>;
} cpsp;

